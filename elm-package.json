{
    "version": "1.0.0",
    "summary": "A opinionated library for plotting lines charts in SVG.",
    "repository": "https://github.com/terezka/line-charts.git",
    "license": "BSD3",
    "source-directories": [
      "./src",
      "./docs/src"
    ],
    "exposed-modules": [
        "Chart.Axis",
        "Chart.Axis.Independent",
        "Chart.Axis.Dependent",
        "Chart.Axis.Intersection",
        "Chart.Axis.Unit",
        "Chart.Axis.Line",
        "Chart.Axis.Range",
        "Chart.Axis.Tick",
        "Chart.Axis.Ticks",
        "Chart.Axis.Title",
        "Chart.Axis.Values",
        "Chart.Colors",
        "Chart.Container",
        "Chart.Coordinate",
        "Chart.Dot",
        "Chart.Events",
        "Chart.Grid",
        "Chart.Interpolation",
        "Chart.Junk",
        "Chart.Legends",
        "Chart.Lines",
        "Chart.Line",
        "Chart.Area",
        "Chart.Dots",
        "Chart.Group",
        "Chart.Trend",
        "Chart.Outliers",
        "Chart.Blocks",
        "Chart.Block",
        "Chart.Pattern"
    ],
    "dependencies": {
        "BrianHicks/elm-trend": "2.1.1 <= v < 3.0.0",
        "debois/elm-dom": "1.2.3 <= v < 2.0.0",
        "elm-lang/core": "5.1.1 <= v < 6.0.0",
        "elm-lang/html": "2.0.0 <= v < 3.0.0",
        "elm-lang/svg": "2.0.0 <= v < 3.0.0",
        "eskimoblood/elm-color-extra": "5.0.0 <= v < 6.0.0",
        "justinmimbs/elm-date-extra": "2.0.3 <= v < 3.0.0",
        "mgold/elm-date-format": "1.4.2 <= v < 2.0.0",
        "myrho/elm-round": "1.0.0 <= v < 2.0.0"
    },
    "elm-version": "0.18.0 <= v < 0.19.0"
}
